{
    "version": 2,
    "routes": [
      {
        "src": "/register",
        "dest": "/api/app.js"
      },
      {
        "src": "/login",
        "dest": "/api/app.js"
      },
      {
        "src": "/users",
        "dest": "/api/app.js"
      },
      
      {
        "src": "/updateuser",
        "dest": "/api/app.js"
      },
      {
        "src": "/users",
        "methods": ["GET", "PUT", "POST" , "UPDATE", "DELETE"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      },
      {
        "src": "/updateuser",
        "methods": ["GET", "PUT", "POST" , "UPDATE", "DELETE"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      },
      {
        "src": "/register",
        "methods": ["GET", "PUT", "POST" , "UPDATE", "DELETE"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      },
  
      {
        "src": "/api/(.*)",
        "dest": "/api/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "/api/app.js"
      }
    ]
  }