{
    "version": 2,
    "builds": [
        {
          "src": "src/app.js",
          "use": "@vercel/node"
        }
      ],
    "routes": [
      {
        "src": "/register",
        "dest": "/api/src/app.js"
      },
      {
        "src": "/login",
        "dest": "/api/src/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  