{
    "version": 2,
  
    "routes": [
      {
        "src": "/register",
        "dest": "/api/src/app.js"
      },
      {
        "src": "/login",
        "dest": "/api/src/app.js"
      },
      {
        "src": "/users",
        "dest": "/api/src/app.js"
      },
      {
        "src": "/users",
        "methods": ["GET", "PUT", "POST" , "UPDATE", "DELETE"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }
  